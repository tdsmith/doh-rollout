	},
	"homepage": "https://github.com/mozilla/doh-rollout#readme",
	"dependencies": {
		"shield-studies-addon-utils": "^5.2.1",
		"web-ext": "^3.1.1"
	},
	"devDependencies": {
		"eslint": "^4.19.1"
	}
